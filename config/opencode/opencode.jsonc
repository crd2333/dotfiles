{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "oh-my-opencode"
  ],
  "keybinds": {
    // disable terminal suspend or exit (as ctrl+d already exists)
    "terminal_suspend": "none",
    "app_exit": "ctrl+d,<leader>q",
    "input_clear": "none", // default is ctrl+c, will conflict with copy selected action
    "input_delete": "delete,shift+delete", // delete ctrl+d to keep ctrl+d single as exit (useless, bug?)
  },
  "provider": {
    "gcli-antigravity": {
      "models": {
        "gemini-3-flash": {
        "name": "Gemini 3 Flash (antigravity)",
        "limit": { "context": 1048576, "output": 65536 },
        "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
        "variants": {
            "minimal": { "thinkingLevel": "minimal" },
            "low": { "thinkingLevel": "low" },
            "medium": { "thinkingLevel": "medium" },
            "high": { "thinkingLevel": "high" },
        }
        },
        "gemini-3-pro-high": {
          "name": "Gemini 3 Pro (antigravity)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingLevel": "low" },
            "high": { "thinkingLevel": "high" },
          }
        },
        "claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          }
        },
        "claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (antigravity)",
          "limit": { "context": 200000, "output": 64000 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "low": { "thinkingConfig": { "thinkingBudget": 8192 } },
            "max": { "thinkingConfig": { "thinkingBudget": 32768 } },
          }
        },
        "gpt-oss-120b-medium": {
          "name": "GPT-OSS 120B Medium (antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-2.5-flash-lite": {
          "name": "Gemini 2.5 Flash Lite (antigravity)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        }
      },
      "name": "gcli-antigravity",
      "npm": "@ai-sdk/google",
      "options": {
        "baseURL": "https://gcli2api4crd233.zeabur.app/antigravity/v1"
      }
    },
    "gcli": {
      "models": {
        "gemini-2.5-flash": {
          "name": "Gemini 2.5 Flash (gcli)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-2.5-pro": {
          "name": "Gemini 2.5 Pro (gcli)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] }
        },
        "gemini-3-flash-preview": {
          "name": "Gemini 3 Flash Preview (gcli)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "high": {"thinkingConfig": {"includeThoughts": true, "thinkingLevel": "high"}},
            "low": {"thinkingConfig": {"includeThoughts": true, "thinkingLevel": "low"}},
            "medium": {"thinkingConfig": {"includeThoughts": true, "thinkingLevel": "medium"}},
          }
        },
        "gemini-3-pro-preview": {
          "name": "Gemini 3 Pro Preview (gcli)",
          "limit": { "context": 1048576, "output": 65535 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text"] },
          "variants": {
            "high": {"thinkingConfig": {"includeThoughts": true, "thinkingLevel": "high"}},
            "low": {"thinkingConfig": {"includeThoughts": true, "thinkingLevel": "low"}},
          }
        },
        "gemini-2.5-flash-image": {
          "name": "Gemini 2.5 Flash Image (gcli)",
          "limit": { "context": 1048576, "output": 65536 },
          "modalities": { "input": ["text", "image", "pdf"], "output": ["text", "image"] }
        }
      },
      "name": "gcli",
      "npm": "@ai-sdk/google",
      "options": {
        "baseURL": "https://gcli2api4crd233.zeabur.app/v1"
      }
    }
  }
}